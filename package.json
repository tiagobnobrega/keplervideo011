{
  "name": "@amzn/Keplervideoapp",
  "version": "2.0.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf build bundle kepler-build node_modules package-lock.json generated coverage",
    "build": "npm-run-all compile lint:fix test",
    "compile": "tsc -p tsconfig.json",
    "test:snapshot": "jest --colors --updateSnapshot",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --colors --coverage",
    "test:clearCache": "jest --clearCache",
    "lint": "eslint src --ext .ts,.tsx && eslint tst --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix && eslint tst --ext .ts,.tsx --fix",
    "start:simulator": "kepler device simulator start",
    "app:install": "kepler device install-app --dir .",
    "app:launch": "kepler device launch-app",
    "app:build:debug": "run-s build:debug && npm run app:install && npm run app:launch -- -b Debug",
    "app:build": "run-s build:release && npm run app:install && npm run app:launch -- -b Release",
    "create-bundle-dirs:debug": "mkdir -p build/lib/rn-bundles/Debug/",
    "bundle-kepler:debug": "react-native bundle --platform kepler --entry-file index.js --bundle-output build/lib/rn-bundles/Debug/index.bundle --assets-dest build/lib/rn-bundles/Debug/ --sourcemap-output build/lib/rn-bundles/Debug/index.bundle.map",
    "hermes-kepler:debug": "react-native hermes-kepler --bundle-path build/lib/rn-bundles/Debug/index.bundle --out build/lib/rn-bundles/Debug/index.hermes.bundle",
    "build:js:debug": "run-s create-bundle-dirs:debug bundle-kepler:debug hermes-kepler:debug bundle-kepler:task:debug hermes-kepler:task:debug",
    "create-bundle-dirs:release": "mkdir -p build/lib/rn-bundles/Release/ && mkdirp build/private/debugging/Release/",
    "bundle-kepler:release": "react-native bundle --platform kepler --entry-file index.js --dev false --bundle-output build/lib/rn-bundles/Release/index.bundle --assets-dest build/lib/rn-bundles/Release/ --sourcemap-output build/private/debugging/Release/index.bundle.map",
    "hermes-kepler:release": "react-native hermes-kepler --bundle-path build/lib/rn-bundles/Release/index.bundle --out build/lib/rn-bundles/Release/index.hermes.bundle",
    "build:js:release": "run-s create-bundle-dirs:release bundle-kepler:release hermes-kepler:release bundle-kepler:task:release hermes-kepler:task:release",
    "build:native:debug": "kepler build -b Debug",
    "build:debug": "run-s build:js:debug build:native:debug",
    "build:native:release": "kepler build -b Release",
    "build:release": "run-s build:js:release build:native:release",
    "build:app": "run-s build build:debug build:release",
    "bundle-kepler:task:debug": "react-native bundle --platform kepler --entry-file task.js --bundle-output build/lib/rn-bundles/Debug/task.bundle --assets-dest build/lib/rn-bundles/Debug/ --sourcemap-output build/lib/rn-bundles/Debug/task.bundle.map",
    "hermes-kepler:task:debug": "react-native hermes-kepler --bundle-path build/lib/rn-bundles/Debug/task.bundle --out build/lib/rn-bundles/Debug/task.hermes.bundle",
    "bundle-kepler:task:release": "react-native bundle --platform kepler --entry-file task.js --dev false --bundle-output build/lib/rn-bundles/Release/task.bundle --assets-dest build/lib/rn-bundles/Release/ --sourcemap-output build/private/debugging/Release/task.bundle.map",
    "hermes-kepler:task:release": "react-native hermes-kepler --bundle-path build/lib/rn-bundles/Release/task.bundle --out build/lib/rn-bundles/Release/task.hermes.bundle",
    "build:native": "kepler build -b Debug -b Release",
    "build:module": "run-s build build:native",
    "prepack": "run-s build:module"
  },
  "dependencies": {
    "@amzn/headless-task-manager": "^1.0.0",
    "@amzn/kepler-channel": "^1.0.0",
    "@amzn/kepler-content-personalization": "^1.0.0",
    "@amzn/kepler-epg-provider": "^1.0.0",
    "@amzn/kepler-epg-sync-scheduler": "^1.0.0",
    "@amzn/kepler-file-system": "~0.0.1",
    "@amzn/kepler-media-account-login": "~1.0.0",
    "@amzn/kepler-media-content-launcher": "~1.0.1",
    "@amzn/kepler-subscription-entitlement": "^1.0.0",
    "@amzn/kepler-ui-components": "~2.0.0",
    "@amzn/keplerblocks-types": "^1.1.0",
    "@amzn/keplerblocks-ui": "^2.1.1",
    "@amzn/keplerscript-appstore-iap-lib": "~2.1.7",
    "@amzn/keplerscript-netmgr-lib": "~2.0.2",
    "@amzn/keplerscript-react-native-reanimated": "~2.0.0",
    "@amzn/lottie-react-native": "~2.0.0",
    "@amzn/package-manager-lib": "~1.0.0",
    "@amzn/react-linear-gradient": "~2.0.0",
    "@amzn/react-native-device-info": "~2.0.0",
    "@amzn/react-native-kepler": "~2.0.0",
    "@amzn/react-native-localize": "~2.0.0",
    "@amzn/react-native-safe-area-context": "~2.0.0",
    "@amzn/react-native-screens": "~2.0.0",
    "@amzn/react-native-svg": "~2.0.0",
    "@amzn/react-native-vector-icons": "~2.0.0",
    "@amzn/react-native-w3cmedia": "~2.1.0",
    "@amzn/react-navigation__core": "~2.0.0",
    "@amzn/react-navigation__drawer": "~2.0.0",
    "@amzn/react-navigation__native": "~2.0.0",
    "@amzn/react-navigation__routers": "~2.0.0",
    "@amzn/react-navigation__stack": "~2.0.0",
    "@jest/globals": "^29.7.0",
    "@react-native-async-storage/async-storage": "npm:@amzn/react-native-async-storage__async-storage@~2.0.0",
    "@reduxjs/toolkit": "~2.2.4",
    "@types/react-test-renderer": "~18.3.0",
    "abortcontroller-polyfill": "~1.7.5",
    "base-64": "1.0.0",
    "fastestsmallesttextencoderdecoder": "~1.0.22",
    "formik": "~2.4.6",
    "jest-fetch-mock": "~3.0.3",
    "lodash": "~4.17.21",
    "newrelic-kepler-agent": "file:packages/newrelic-kepler-agent-0.8.0.tgz",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-event-listeners": "~1.0.7",
    "react-native-gesture-handler": "npm:@amzn/react-native-gesture-handler@~2.0.0",
    "react-native-responsive-screen": "~1.4.2",
    "react-native-uuid": ">=2.0.1",
    "react-redux": "~9.1.2",
    "recyclerlistview": "~4.2.0",
    "web-streams-polyfill": "^3.2.1",
    "xmldom": "~0.6.0",
    "yup": "~1.4.0"
  },
  "devDependencies": {
    "@amzn/kepler-cli-platform": "^0",
    "@amzn/kepler-performance-api": "^0.0.1",
    "@babel/core": "~7.22.8",
    "@babel/runtime": "~7.22.8",
    "@react-native-community/cli": "11.3.2",
    "@react-native-community/eslint-config": "~2.0.0",
    "@react-native/eslint-config": "~0.72.2",
    "@react-native/metro-config": "~0.72.6",
    "@testing-library/jest-native": "~5.4.3",
    "@testing-library/react-native": "~11.5.4",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/base-64": "~1.0.2",
    "@types/jest": "~28.1.8",
    "@types/lodash": "~4.17.0",
    "@types/react": "~18.2.75",
    "@types/redux-mock-store": "^1.0.6",
    "@types/xmldom": "~0.1.34",
    "@typescript-eslint/eslint-plugin": "~6.10.0",
    "babel-jest": "~28.1.3",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-simple-import-sort": "~5.0.3",
    "jest": "~28.1.3",
    "metro-react-native-babel-preset": "~0.76.9",
    "mustache": "~4.2.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.8.8",
    "prettier-plugin-organize-imports": "~3.2.4",
    "react-native-dotenv": "~3.4.11",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "^1.5.4",
    "run-s": "^0.0.0",
    "ts-jest": "~28.0.8",
    "typescript": "~4.9.5"
  },
  "overrides": {
    "react-native-gesture-handler": "$react-native-gesture-handler",
    "@react-native-async-storage/async-storage": "npm:@amzn/react-native-async-storage__async-storage@~2.0.0"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "eslintConfig": {
    "extends": "@react-native",
    "root": true,
    "ignorePatterns": [
      "src/w3cmedia/shakaplayer/dist/"
    ]
  },
  "kepler": {
    "projectType": "application",
    "appName": "Keplervideoapp",
    "api": 0.1,
    "targets": [
      "tv"
    ],
    "features": [
      "vega"
    ],
    "experimental": {
      "decoupleJS": true
    }
  }
}
